{
    "entry_point": "src/training/train_sasrec_dp.py",
    "dataset": "retailrocket_views",
    "num_days": 4,
    "parameters": 
    {
        "max-epochs": "1000",
        "batch-size": "128",
        "effective-batch-size":"128",
        "repetitions-filter": "False",
        "early-stop-on":"none",
